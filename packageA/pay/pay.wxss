
.wrapper{
    min-height: 100vh;
    background: #f6f6f6;
    padding-bottom: 60px;
}
.name-wrapper{
    height: 80rpx;
    line-height: 40rpx;
    margin-bottom: 20rpx;
}
.m-r5{
    margin-right: 18rpx;
}

.icon{
    width: 28rpx;
    height: 28rpx;
    vertical-align: middle;
    margin-right: 3px;
}

.footer{
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 10;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1px solid #f6f6f6;
}

.info-wrapper{
    padding: 20rpx 40rpx;
    margin-bottom: 6px;
}

.title{
    padding: 18rpx 0;
}

.item-wrapper{
    line-height: 32rpx;
    padding: 24rpx 0;
    border-top: 1px solid #f6f6f6;
}

.number-wrapper image{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin: 0 16rpx;
}